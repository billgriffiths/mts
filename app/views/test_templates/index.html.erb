<h1>Listing test_templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
	  <tr>
	    <td>Name</td>
	    <td>Description</td>
	  </tr>
    <% @test_templates.each do |test_template| %>
      <tr>
 	    <td><%= test_template.name%></td>
 	    <td><%= test_template.description%></td>
        <td><%= link_to 'Show', controller: "test_templates", action: "show", id: test_template  %></td>
        <td><%= link_to 'Edit', controller: "test_templates", action: "edit", id: test_template %></td>
        <td><%= link_to 'Destroy', controller: "test_templates", action: "destroy", id: test_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
